    "@nestjs/core": "^7.6.1",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/terminus": "^7.0.1",
    "cheerio": "^1.0.0-rc.9",
    "compression": "^1.7.4",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "got": "^11.8.2",
    "helmet": "^4.2.0",
    "i18n": "^0.13.2",
    "joi-objectid": "^3.0.1",

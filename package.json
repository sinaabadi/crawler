    "rimraf": "^3.0.0",
    "rxjs": "^6.6.3",
    "sitemapper": "^3.1.12",
    "swagger-ui-express": "^4.1.5",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",

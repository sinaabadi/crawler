    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.1",
    "@types/config": "^0.0.37",
    "@types/express": "^4.17.9",
    "@types/i18n": "^0.12.0",
    "@types/jest": "26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
